@addTagHelper "*, Microsoft.AspNet.Mvc.TagHelpers"
<!DOCTYPE html>
<html lang="ru" class="uk-height-1-1">
<head>
    <base href="/"/>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="/favicon.png">
    <title>Регистрация в объединения - ЦДЮТТиИТ</title>
    <environment names="Development">
        <link href="~/css/style.css" rel="stylesheet">
    </environment>
    <environment names="Staging, Production">
        <link href="~/css/style.css" rel="stylesheet">
    </environment>

    @*<script src="~/lib/pace/pace.min.js"></script>*@
</head>
<body class="uk-height-1-1">
    <app-component>
        <div class="loading"></div>
    </app-component>

    <script src="~/lib/jquery.min.js"></script>

    <!-- IE required polyfills, in this exact order -->
    <script src="~/lib/angular2/es6-shim.min.js"></script>
    <script src="~/lib/angular2/system-polyfills.js"></script>
    <script src="~/lib/angular2/shims_for_IE.js"></script>

    <script src="~/lib/angular2/angular2-polyfills.js"></script>
    <script src="~/lib/angular2/system.src.js"></script>
    <script src="~/lib/angular2/Rx.js"></script>
    <script src="~/lib/angular2/angular2.dev.js"></script>

    <script src="~/lib/angular2/router.dev.js"></script>
    <script src="~/lib/angular2/http.js"></script>

    <script>
        System.config({
            packages: {
                app: {
                    format: 'register',
                    defaultExtension: 'js'
                },
                'rxjs': { defaultExtension: 'js' }
            },
            map: {
                rxjs: 'lib/rxjs'
            }
        });
        System.import("app/main").then(null, console.error.bind(console));;
    </script>


</body>
</html>